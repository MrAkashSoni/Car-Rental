<body>
   <section class="hero-wrap hero-wrap-2 js-fullheight" style="background-image: url('public/images/bg_3.jpg');" data-stellar-background-ratio="0.5">
      <div class="overlay"></div>
      <div class="container">
        <div class="row no-gutters slider-text js-fullheight align-items-end justify-content-start">
          <div class="col-md-9 ftco-animate pb-5">
          	<p class="breadcrumbs"><span class="mr-2"><a href="/">sub admin <i class="ion-ios-arrow-forward"></i></a></span> <span>Owned Cars <i class="ion-ios-arrow-forward"></i></span></p>
            <h1 class="mb-3 bread">Owned Car</h1>
          </div>
        </div>
      </div>
    </section>
    <section class="ftco-section bg-light">
    	<div class="container">
    		<div class="row">
          {{#each ownedCars}}
    			<div class="col-md-4">
    				<div class="car-wrap rounded ftco-animate">
    					<div class="img rounded d-flex align-items-end" style='background-image: url("/images/carImages/{{this.image}}");'>
    					</div>
    					<div class="text">
    						<h2 class="mb-0"><a href="car-single.html">{{this.model}}</a></h2>
    						<div class="d-flex mb-3">
	    						<span class="cat">{{this.model}}</span>
	    						<p class="price ml-auto">₹ {{this.price}} <span>/day</span></p>
    						</div>
    						<p class="d-flex mb-0 d-block">
                  <a href="/cars/editPrice/{{this._id}}" class="btn btn-primary py-2 mr-1">EDIT DETAIL</a>
                  <a href="/cars/withdrawCar/{{this._id}}" class="btn btn-secondary py-2 ml-1" onclick="return confirm ('Are you sure you want to withraw car from rental service? ');">WITHDRAW CAR</a>
                </p>
    					</div>
    				</div>
    			</div>
          {{/each}}
    		<div class="row mt-5">
          <div class="col text-center">
            <div class="block-27">
              <ul>
                <a href="?page={{pageObj.prevPage}}">Prev page</a>
              </ul>
            </div>
             <div class="block-27">
              <ul>
                <a href="?page={{pageObj.nextPage}}">Next page</a>
              </ul>
            </div>
          </div>
        </div>
    	</div>
    </section>   
</body>



