
<body>
   <section class="hero-wrap hero-wrap-2 js-fullheight" style="background-image: url('public/images/bg_3.jpg');" data-stellar-background-ratio="0.5">
      <div class="overlay"></div>
      <div class="container">
        <div class="row no-gutters slider-text js-fullheight align-items-end justify-content-start">
          <div class="col-md-9 ftco-animate pb-5">
          	<p class="breadcrumbs"><span class="mr-2"><a href="/">Admin <i class="ion-ios-arrow-forward"></i></a></span> <span>Owned Cars <i class="ion-ios-arrow-forward"></i></span></p>
            <h1 class="mb-3 bread">Owner List</h1>
          </div>
        </div>
      </div>
    </section>
    <section class="ftco-section bg-light">
    	<div class="container">
    		<table border="2">
    <tr>
        <th>&emsp;&emsp;Name</th>
        <th>&emsp;&emsp;Email</th>
        <th>&emsp;&emsp;Rented Car Name</th>
        <th>&emsp;&emsp;Passing Number</th>
    </tr>
    {{#each list}}
    {{log list}}
    <tr>
        <td>&emsp;&emsp;{{this.ownerID.firstName}} {{this.ownerID.lastName}}</td>
        <td>&emsp;&emsp;{{this.ownerID.email}}</td>
        <td>&emsp;&emsp;{{this.model}}</td>
        <td>&emsp;&emsp;{{this.passingNumber}}</td>

    </tr>
    {{/each}}
</table>
          <div class="col text-center">
    
          </div>
        </div>
    	</div>
    </section>   
</body>