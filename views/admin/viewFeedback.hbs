<body>
   <section class="hero-wrap hero-wrap-2 js-fullheight" style="background-image: url('public/images/bg_3.jpg');" data-stellar-background-ratio="0.5">
      <div class="overlay"></div>
      <div class="container">
        <div class="row no-gutters slider-text js-fullheight align-items-end justify-content-start">
          <div class="col-md-9 ftco-animate pb-5">
          	<p class="breadcrumbs"><span class="mr-2"><a href="/"> admin <i class="ion-ios-arrow-forward"></i></a></span> <span>Owned Cars <i class="ion-ios-arrow-forward"></i></span></p>
            <h1 class="mb-3 bread">Messages</h1>
          </div>
        </div>
      </div>
    </section>
    <section class="ftco-section bg-light">
    	<div class="container">
    		<div class="row">
            {{#each feedbackMessage}}
            {{log feedbackMessage}}
                Name
                <div class="form-group">
                    <input type="text" class="form-control"value="{{feedbackMessage.name}}" readonly>
                </div>
                Email
                <div class="form-group">
                    <input type="text" class="form-control" value="{{feedbackMessage.email}}" readonly>
                </div>
                Subject
                <div class="form-group">
                    <input type="text" class="form-control" value="{{feedbackMessage.subject}}" readonly>
                </div>     
                Message       
                <div class="form-group">
                    <input type="text" class="form-control" value="{{feedbackMessage.message}}" readonly>
                </div>
            {{/each}}
    	</div>
    </section>   
</body>